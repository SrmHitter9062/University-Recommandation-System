      err = loginFnc.msgPasswordsDontMatch;
    }else if (!loginFnc.checkEmail(trim(login))){
      err = loginFnc.msgIncorrectEmail + ' ' + loginFnc.msgTryAgain;
    }

    $('#loginreg-div  #loginErrorMsg').html('').hide();
    $('#loginreg-div  #signupErrorMsg').html(err);
    if(err){
      $('#loginreg-div #signupErrorMsg').show();
      return false;
    }else{
      $('#loginreg-div  #signupErrorMsg').hide();
    }

    var params = {
      login: login,
      password: password,
      password2: password2,
      successUrl: unescape(paramsObj.returnTo)
    };

    $("body").css("cursor", "progress");
    $.get('https://www.4shared.com/signup?callback=?',
      params,
      function(data){
        $("body").css("cursor", "auto");
        if(data.ok){
          authSaveParams(paramsObj);
          loginFnc.redirect(data.redirectUrl || loginFnc.getLocation());
        }else{
          $('#loginreg-div  #signupErrorMsg').html(data.error).show();
        }
      },
      "jsonp"
    );
    return false;
  }

