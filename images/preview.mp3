<script type="text/javascript">

    


  function showShortNotification(){
    $('#nloader').show();
    $.ajax({
        type: 'get',
        dataType: 'json',
        url: "/web/notification/showshort",
        success: function(data) {
          var fullhtml="";
          var list=data.infolist;
          var unreadCount=0;

          if (list.length>0) {
            $('#nonotifications').hide();
            $('#ncontent').show();
          }

          for (var i=0; i<list.length; i++) {
            var event = list[i];
            var eventType = event.type;
            var avatar = event.fromUserAvatarLink;
            if (eventType=="SHARE_FILE" || eventType=="SHARE_FOLDER") {
              eventType="tshare";
            }
            else if (eventType=="COMMENT") {
              eventType="tcomment";
            } else if (eventType=="BACKGROUND_FILE_TASK") {
              eventType="tbgtask";
              var ext = event.iFileName.split('.').pop()
              if(ext == 'zip') avatar="http://static.4shared.com/icons/32x32/zip.png?ver=655479399"
              else if (ext == 'b1') avatar="http://static.4shared.com/icons/32x32/b1.png?ver=703417425"
              else if (ext == 'torrent') avatar="http://static.4shared.com/icons/32x32/torrent.png?ver=1628575404"
              event.fromUserName = "Your file is ready for download:"
            } else if(eventType=="CHAT_MESSAGE"){
              eventType="tmessage";
            }
            var unread="";
            if (event.status=='NEW') {
              unread = "unread";
              unreadCount++;
            }

            if (avatar == "default")
              avatar = "http://static.4shared.com/images/user2.png";
            var info2Text = "";
            if (event.info2 != ""){
                 info2Text =  "<br /><i class='f9 paddingT5'>"+event.info2+"</i>";
            }
            var html = "<div onclick=\"doAction('"+event.actionLink+"')\" class='nitem "+unread+" clear paddingLR5'>"+
                "<div class='uPhoto floatLeft'><img id='userAvatarImage' src='"+avatar+"' width='32' height='32'></div>"+
                "<div class='descr floatLeft f11 marginL10 ffshadow'>"+
                  "<u>"+event.fromUserName+"</u> "+
                  event.info1+" "+
                  "<u>"+event.iFileName+"</u>. "
                  +info2Text+
                "</div>"+
                "<div class='type floatLeft sprite1 "+eventType+"'></div>"+
              "</div>";
            fullhtml = fullhtml + html;
          }
          $('#nloader').hide();
          $("#nscroll").html(fullhtml);
          $(".ncounter").text(unreadCount);
          if (unreadCount>0) {
            $(".ncounter").show();
          }
          if($("#nscroll").height() > 199) {
            $("#nscroll").slimScroll({height: '200px'})
          }
        },
        error: function() {
            $('#nloader').hide();
            $('#ncontent').hide();
            $(".ncounter").hide();
            $('#nonotifications').show();
        }
    })
  }


  function showFullNotification(page) {
    location.href = "/web/notification/showall?page=" + page;
  }

  function doAction(link) {
    location.href = link;
