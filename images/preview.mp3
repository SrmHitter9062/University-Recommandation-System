  function getNotificationCount() {
    if($(".cnotifications").css("display")=="block")
      return;

    $.ajax({
      type: 'get',
      dataType: 'json',
      url: "/web/notification/count",
      success: function(data) {
        if (data.unread>0) {
          $(".ncounter").text(data.unread);
          $(".ncounter").show();
          Events.fireEvent("new.notification");
        }
      },
      error: function() {
        //console.log("Error. includeNewHeader.jsp -> getNotificationCount()")
      }
    })
  }

  $(document).click(function(event) {
    if($(".cnotifications").css("display")=="block") {
      $(".ncounter").text("0");
      $(".ncounter").hide();
    }
  })


  setInterval('getNotificationCount();', 600000);
</script>
     

